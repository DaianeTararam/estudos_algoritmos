algoritmo "Antecessor e sucessor"
var
   numero, antecessor, sucessor: Inteiro
inicio
   Escreva("Digite um numero inteiro: ")
   leia (numero)
   antecessor <- numero - 1
   sucessor <- numero + 1
   Escreval("O número antecessor é ", antecessor)
   Escreval("O número suceesor é ", sucessor)

fimalgoritmo