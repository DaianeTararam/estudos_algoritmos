algoritmo "Descubra o valor de delta"
var
   a, b, c, delta: Inteiro
inicio
   Escreva("Digite os valores de a, b e c.")
   EScreval("A: ")
   leia(a)
   Escreval("B: ")
   leia(b)
   Escreval("C: ")
   leia(c)
   delta <- b * b - 4 * a * c
   
   Escreval("O valor de delta é: ", delta)
fimalgoritmo